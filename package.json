{
  "name": "diviner-archive",
  "version": "0.5",
  "description": "A WordPress Plugin for non-profits archives",
  "main": "webpack.mix.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
